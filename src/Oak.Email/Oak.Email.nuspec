<?xml version="1.0"?>
<package >
  <metadata>
    <id>Oak.Email</id>
    <version>0.0.3-preview2</version>
    <title>Oak.Email</title>
    <authors>Chris (@ChrisWeaver1)</authors>
    <owners>Chris (@ChrisWeaver1)</owners>
    <description>Oak email client library</description>
    <repository type="git" url="https://github.com/weavc/Oak.git"/>
    <dependencies>
      <group targetFramework="net5.0">
        <dependency id="MailKit" version="2.10.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Hosting" version="5.0.0" exclude="Build,Analyzers" />
        <dependency id="Sendgrid" version="9.24.3" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.1">
        <dependency id="MailKit" version="2.10.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Hosting" version="5.0.0" exclude="Build,Analyzers" />
        <dependency id="Sendgrid" version="9.24.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
   <files>
      <file src="bin\Release\netstandard2.1\**\*" target="lib\netstandard2.1" />
      <file src="bin\Release\net5.0\**\*" target="lib\net5.0" />
  </files>
</package>